<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="androidlab.epam.com.task2module">
    <permission android:name="androidlab.epam.com.task2module.permission.START_ACTIVITY"
        android:label="@string/permiss_label"
        android:description="@string/permiss_description"
        android:permissionGroup="android.permission-group.START"
       android:protectionLevel="normal"
        />

    <application android:allowBackup="true" android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name" android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true" android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:permission="androidlab.epam.com.task2module.permission.START_ACTIVITY">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>